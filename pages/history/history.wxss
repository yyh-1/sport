.top1{
  height: 7%;
  width: 50%;
  /* background-color: tomato; */
  display: flex;
  align-items: center;
  float: left;
}
.top1 text{
  position: relative;
  left: calc(50% - 48rpx);
}
.top2{
  height: 7%;
  width: 50%;
  /* background-color: tomato; */
  display: flex;
  align-items: center;

}
.top2 text{
  position: relative;
  left: calc(50% - 48rpx);
}
.body{
  width: 100%;
  height: 93%;
  background-color: #f5f5f5;
}
.body .content{
  position: relative;
  top: 20rpx;
  width: 90%;
  height: 30%;
  background-color: #fff;
  border-radius: 8px;
  margin: auto;
  margin-top: 20rpx;
}
.body .content .contentList{
  height: 25%;
  width: 90%;
  margin: auto;
  border-bottom: 1px solid #f5f5f5;
  display: flex;
  align-items: center;
}
.body .content .contentList image{
  width: 35rpx;
  height: 35rpx;
}
.body .content .contentList .text1{
  margin-left: 20rpx;
  color: #666666;
}
.body .content .contentList .text2{
  position: absolute;
  right: 8%;
  color: #666666;
}


